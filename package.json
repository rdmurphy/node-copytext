{
  "name": "copytext",
  "version": "1.1.0",
  "description": "A module for accessing a XLSX spreadsheet as a JavaScript object.",
  "main": "index.js",
  "files": [
    "index.js",
    "process.js",
    "processors"
  ],
  "scripts": {
    "docs:build": "documentation readme --readme-file README.md --section 'API Docs' ./index.js && doctoc --github README.md",
