  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdmurphy/node-copytext/issues"
  },
  "homepage": "https://github.com/rdmurphy/node-copytext",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "xlsx": "^0.8.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "doctoc": "^1.2.0",
    "documentation": "^4.0.0-beta5",
    "istanbul": "^0.4.4",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "^1.2.0",
    "standard": "^7.1.2"
  }
