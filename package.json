{
  "name": "copytext",
  "version": "2.1.0",
  "description": "A module for accessing a XLSX spreadsheet as a JavaScript object.",
  "main": "index.js",
  "files": [
    "index.js",
    "processors"
  ],
  "scripts": {
    "build": "NODE_ENV=production typebundle src/index.ts --output dist --target 8 --compress",
    "docs:build": "documentation readme --readme-file README.md --section 'API Docs' ./index.js && doctoc --github README.md",
    "release": "np --no-yarn",
    "test": "NODE_ENV=test mocha --require test/setup.js test/test.js",
    "test:cov": "NODE_ENV=test nyc --extension ts --require ./test/setup.js mocha test/test.js",
    "test:coveralls": "NODE_ENV=test nyc report --reporter text-lcov --exclude-after-remap false | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdmurphy/node-copytext"
  },
  "keywords": [
    "json",
    "xlsx",
    "excel",
    "templates"
  ],
  "author": "Ryan Murphy <ryan@rdmurphy.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdmurphy/node-copytext/issues"
  },
  "homepage": "https://github.com/rdmurphy/node-copytext",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@newswire/prettier-config": "^2.0.0",
    "@types/node": "^12.7.2",
    "@zeit/git-hooks": "^0.1.4",
    "babel-plugin-istanbul": "^5.2.0",
    "coveralls": "^3.0.6",
    "doctoc": "^1.2.0",
    "documentation": "^12.1.1",
    "mocha": "^6.2.0",
    "np": "^5.0.3",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "typebundle": "^0.5.0"
  },
  "prettier": "@newswire/prettier-config"
}
